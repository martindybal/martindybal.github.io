{
  "version": "1.0.0",
  "name": "martin.dybal.personal.website",
  "private": true,
  "devDependencies": {
    "browser-sync": "^2.23.7",
    "node-sass": "4.5.3",
    "watch": "1.0.2"
  },
  "scripts": {
    "browser-sync": "browser-sync start --server --files \"output/css/*.css\" \"*.html\"",
    "compile:scss": "node-sass Styles/style.scss -o output/css/",
    "watch:scss": "watch \"npm run compile:scss\" Styles"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "browser-sync",
      "watch:scss"
    ]
  }
}
